{
    "name": "waygou/xheetah",
    "description": "Laravel Nova admin framework for Courier transports management.",
    "type": "library",
    "keywords": [
        "utilities",
        "xheetah",
        "courier",
        "management",
        "waygou",
        "laravel",
        "php"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.6.*|5.7.*",
        "tightenco/parental": "dev-master",
        "symfony/process": "^4.1",
        "predis/predis": "^1.1",
        "pelmered/fake-car": "^1.0",
        "fzaninotto/faker": "^1.8",
        "hyn/multi-tenant": "^5.3",
        "waygou/helpers": "dev-master",
        "waygou/xheetah-nova": "dev-master",
        "waygou/surveyor": "dev-master",
        "waygou/surveyor-nova": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\Xheetah\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\Xheetah\\XheetahServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "minimum-stability": "dev"
}
